/*******************************
         Site Overrides
*******************************/

@font-path : '/client/src/styles/assets/fonts';

@font-face {
    font-family: 'PF Din Text Comp Pro';
    src: url('@{font-path}/pfdintextcomppro-regular.ttf') format('ttf');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'PF Din Text Comp Pro Bold';
    src: url('@{font-path}/pfdintextcomppro-bold.ttf') format('ttf');
    font-weight: normal;
    font-style: normal;
}
