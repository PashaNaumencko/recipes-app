{
  "name": "server",
  "version": "0.0.0",
  "main": "server.ts",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "express-fileupload": "^1.1.6-alpha.5",
    "mongoose": "^5.9.20",
    "multiparty": "^4.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express-fileupload": "^1.1.3",
    "@types/mongoose": "^5.7.30",
    "@types/node": "^14.0.20",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^1.19.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  }
}
